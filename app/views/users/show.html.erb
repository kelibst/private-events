<div class="container">
<h1 class="text-center p-5">Welcome to <%= @user.username%>'s page.</h1>


    <h4 class="text-center"><%= @user.username%>'s Events</h4>

   <%= render 'events/event', obj: @user.events %>

   

<%= link_to "Create Event", new_event_path, class: "btn btn-success"%>
    <% if logged_in? && current_user == @user %>
        <%= link_to "Log out", logout_path, method: :delete, class: "btn btn-danger"%>
    <% end %>
</div>



